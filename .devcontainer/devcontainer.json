{
  "name": "veen.com Jekyll",
  "image": "mcr.microsoft.com/devcontainers/ruby:3.1",
  "remoteUser": "vscode",
  "postCreateCommand": "bash .devcontainer/post-create.sh",
  "postStartCommand": "bash .devcontainer/post-start.sh",
  "containerEnv": {
    "JEKYLL_ENV": "development"
  },
  "forwardPorts": [4000],
  "portsAttributes": {
    "4000": {
      "label": "Jekyll dev server"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "rebornix.Ruby",
        "castwide.solargraph"
      ]
    }
  }
}

---
layout: null
permalink: /jeff/rss-source.xml
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  {% assign latest_post = site.posts | first %}
  <title>{{ site.title | default: "Jeffrey Veen" }}</title>
  <subtitle>{{ site.description | default: "The personal web site of Jeffrey Veen." }}</subtitle>
  <link href="{{ '/jeff/rss-source.xml' | absolute_url }}" rel="self" type="application/atom+xml" />
  <link href="{{ '/jeff/' | absolute_url }}" rel="alternate" type="text/html" />
  <id>{{ '/jeff/' | absolute_url }}</id>
  {% if latest_post %}
  <updated>{{ latest_post.date | date_to_xmlschema }}</updated>
  {% else %}
  <updated>{{ "now" | date_to_xmlschema }}</updated>
  {% endif %}
  <author>
    <name>{{ site.author | default: "Jeffrey Veen" }}</name>
  </author>
  {% for post in site.posts limit:20 %}
  <entry>
    <title>{{ post.title | xml_escape }}</title>
    <link href="{{ post.url | absolute_url }}" rel="alternate" type="text/html" />
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>{{ post.url | absolute_url }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>
